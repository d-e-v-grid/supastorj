{
  "name": "@supastorj/cli",
  "version": "0.1.0",
  "description": "Supastorj CLI - DevOps platform for Supabase Storage management",
  "author": "DevGrid",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "supastorj": "./bin/supastorj.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "dev": "tsx watch src/cli/index.ts",
    "build": "tsc",
    "clean": "rimraf dist",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "dependencies": {
    "@clack/prompts": "^0.8.2",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dockerode": "^4.0.2",
    "dotenv": "^16.4.7",
    "execa": "^9.5.1",
    "ink": "^5.0.1",
    "ink-spinner": "^5.0.0",
    "ink-table": "^3.1.0",
    "js-yaml": "^4.1.0",
    "ora": "^8.1.1",
    "react": "^18.3.1",
    "winston": "^3.16.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/dockerode": "^3.3.33",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.6",
    "@types/react": "^18.3.17",
    "@vitest/coverage-v8": "^2.1.8",
    "ink-testing-library": "^4.0.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "supastor",
    "cli",
    "supabase",
    "storage",
    "devops",
    "tui"
  ]
}
