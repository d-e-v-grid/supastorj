{
  "name": "@supastorj/cli",
  "version": "0.1.0",
  "description": "Supastorj CLI - DevOps platform for Supabase Storage management",
  "author": "DevGrid",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "supastorj": "./bin/supastorj.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "dev": "tsx watch src/cli/index.ts",
    "build": "tsc",
    "clean": "rimraf dist",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "chalk": "5.4.1",
    "commander": "14.0.0",
    "dockerode": "4.0.7",
    "dotenv": "17.2.0",
    "ink": "6.0.1",
    "ink-spinner": "^5.0.0",
    "ink-table": "^3.1.0",
    "js-yaml": "^4.1.0",
    "ora": "8.2.0",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "react": "19.1.0",
    "zod": "3.25.76",
    "zx": "8.6.2"
  },
  "devDependencies": {
    "@types/dockerode": "3.3.42",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "24.0.12",
    "@types/pino": "7.0.4",
    "@types/react": "19.1.8",
    "@vitest/coverage-v8": "3.2.4",
    "ink-testing-library": "^4.0.0",
    "rimraf": "6.0.1",
    "tsx": "4.20.3",
    "typescript": "5.8.3",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "supastorj",
    "cli",
    "supabase",
    "storage",
    "devops",
    "tui",
    "docker",
    "postgresql",
    "s3",
    "minio",
    "object-storage"
  ]
}
